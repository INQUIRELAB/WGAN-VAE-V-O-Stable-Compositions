SYSTEM = V1O2
PREC = Accurate
ENCUT = 500
IBRION = 2
ISIF = 3
NSW = 100
EDIFF = 1E-6
EDIFFG = -0.05
ISPIN = 2
LDAU = .TRUE.
LDAUTYPE = 2
LDAUL = 2 -1 # U for V, -1 for O
LDAUU = 3.25 0.0 # U value for V, 0 for O (example value, check Materials Project for exact U)
LDAUJ = 0.0 0.0 # J value
MAGMOM = 1*2.0 2*0.6 # Initial magnetic moments for V and O
ALGO = Fast
NELM = 100
AMIX = 0.2
BMIX = 0.0001
AMIX_MAG = 0.8
